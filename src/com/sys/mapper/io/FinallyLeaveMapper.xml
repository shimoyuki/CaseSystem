<?xml version="1.0" encoding="UTF-8"?>
 <!DOCTYPE mapper PUBLIC 
"-//mybatis.org//DTD mapper 3.0//EN"
"http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.sys.dao.io.FinallyLeaveDao">
	<!-- 保存嫌疑人活动记录 -->
	<insert id="saveFinallyLeave" parameterType="FinallyLeave">
		insert into
	 		finallyleave
	 		(recordNumber,leaveTime,leaveCause,goodsDispose,goodsReceiptor,receiptorID,receiptorTime,leaveAdmin,adminTime)
	 	values(#{recordNumber},#{leaveTime},#{leaveCause},#{goodsDispose},#{goodsReceiptor},#{receiptorID},#{receiptorTime},#{leaveAdmin},#{adminTime})	
	</insert>
</mapper>